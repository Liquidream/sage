{
  "name": "sage",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "test:unit": "vitest --environment jsdom",
    "serve": "vite preview",
    "build": "vite build",
    "build github": "vite build --base=/sage/",
    "deploy github": "gh-pages -d dist",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "localforage": "^1.10.0",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate-2": "^2.0.14",
    "pixi.js": "^6.4.2",
    "roboto-fontface": "*",
    "tweedle.js": "^2.0.0",
    "vue": "^3.2.37",
    "vuetify": "^3.0.0-beta.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.43",
    "@types/webfontloader": "^1.0.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^9.0.0",
    "gh-pages": "^4.0.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "typescript": "~4.7.4",
    "vite": "^2.9.14",
    "vite-plugin-vuetify": "^1.0.0-alpha.0",
    "vitest": "^0.18.0",
    "vue-cli-plugin-vuetify": "~2.5.1",
    "vue-tsc": "^0.38.4"
  }
}
